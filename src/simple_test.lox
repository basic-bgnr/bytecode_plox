var val = 35;

match val {
	case >60      =>  print "greater than 60"
	case >50      =>  print "greater than 50"
	case >40      =>  print "greater than 40"
	case >30      =>  print "greater than 30"
	case >20      =>  print "greater than 20"
	case >10      =>  print "greater than 10"
	case default  =>  print "otherwise"
	
};


if val > 10 {
	print "greater than 10";
};

